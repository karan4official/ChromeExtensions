{
  "manifest_version": 2,

  "name": "FirstRain Rehydrate",
  "description": "This extension shows a FirstRain categories for the article",
  "version": "1.0",

  "browser_action": {
	"default_title": "Rehydrate",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
          "16": "icon.png",
          "48": "icon.png",
          "128": "icon.png"
        },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
